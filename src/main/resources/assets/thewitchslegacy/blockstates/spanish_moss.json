{
  "multipart": [
    {
      "when": { "north": "true" },
      "apply": {
        "model": "thewitchslegacy:block/spanish_moss",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": { "east": "true" },
      "apply": {
        "model": "thewitchslegacy:block/spanish_moss",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": { "south": "true" },
      "apply": {
        "model": "thewitchslegacy:block/spanish_moss"
      }
    },
    {
      "when": { "west": "true" },
      "apply": {
        "model": "thewitchslegacy:block/spanish_moss",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": { "up": "true" },
      "apply": {
        "model": "thewitchslegacy:block/spanish_moss",
        "x": 90,
        "uvlock": true
      }
    }
  ]
}
